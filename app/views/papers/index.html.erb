<%- model_class = Paper -%>
<div class="row">
  <div class="span12">
    <h2>Volume information</h2>

    
  </div>
  <div class="span3">
    <h2>Filter</h2>
    <h3>By year</h3>

  </div>
  <div class="span9">
    <div class="page-header">
      <h2><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h2>
    </div>
    <table class="table table-striped">
      <thead>
        <tr>
          <th><%= model_class.human_attribute_name(:title) %></th>
          <th><%= model_class.human_attribute_name(:month) %></th>
          <th><%= model_class.human_attribute_name(:year) %></th>
          <th><%= model_class.human_attribute_name(:address) %></th>
          <!--<th><%=t '.actions', :default => t("helpers.actions") %></th>-->
        </tr>
      </thead>
      <tbody>
        <% @papers.each do |paper| %>
          <tr>

            <td><%= link_to paper.title, paper_path(paper) %></td>
            <td><%= paper.month %></td>
            <td><%= paper.year %></td>
            <td><%= paper.address %></td>
            <!--<td>
              <%= link_to t('.view', :default => t("helpers.links.view")),
                          paper_path(paper), :class => 'btn btn-mini btn-primary' %>
            <%= link_to t('.edit', :default => t("helpers.links.edit")),
                          edit_paper_path(paper), :class => 'btn btn-mini' %>
            <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                          paper_path(paper),
                          :method => :delete,
                          :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                          :class => 'btn btn-mini btn-danger' %>
            </td>-->
          </tr>
        <% end %>
      </tbody>
    </table>

    <%= link_to t('.new', :default => t("helpers.links.new")),
    new_paper_path,
    :class => 'btn btn-primary' %>
  </div>

</div>

